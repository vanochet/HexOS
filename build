fasm src/boot/bootcode/fat32/boot.asm
fasm src/boot/bootcode/mbr/boot.asm
chown -R talisman src
fasm src/hexos/HXLDR.asm
rm -rf dst/
cp -r src dst
chown -R talisman dst
rm dst/boot/bootcode/fat32/boot.asm
rm dst/boot/bootcode/mbr/boot.asm
rm dst/hexos/HXLDR.asm
./mkimg --dir=dst/hexos --prt=dst/boot/bootcode/fat32/boot.bin --mbr=dst/boot/bootcode/mbr/boot.bin --device=hdd
